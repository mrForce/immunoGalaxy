

<tool id="fa_gc_content_3" name="Upload a file with peptides, and create a venn diagram of overlap with tool output" version="0.1.0">
  <command interpreter="python">overlap.py

  --sample_one '$one'
  --sample_two '$two'
  --output '$output'
  --union '$union'
  --intersection '$intersection'
  --sample_one_unique '$sample_one_unique'
  --sample_two_unique '$sample_two_unique'
  </command>
  <inputs>

    <param name="one" type="data" label="Select sample 1" format="txt" />
    <param name="two" type="data" label="Select sample 2" format="txt" />
  </inputs>
  <outputs>
    <data format="png" name="output" />
    <data format="tabular" name="intersection" />
    <data format="tabular" name="union" />
    <data format="tabular" name="sample_one_unique" />
    <data format="tabular" name="sample_two_unique" />
  </outputs>


  <tests>
    <test>
      <param name="input" value="fa_gc_content_input.fa"/>
      <output name="out_file1" file="fa_gc_content_output.txt"/>
    </test>
  </tests>         
  
  <help>
    This tool computes GC content from a FASTA file.
  </help>

</tool>


