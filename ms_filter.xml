

<tool id="fa_gc_content_2" name="Filter pipeline output by q-value" version="0.1.0">
  <command interpreter="python">filter.py

  --input '$input'
  --threshold '$threshold'
  --output '$output'
  </command>
  <inputs>

    <param name="input" type="data" label="Select pipeline output" format="tabular" />
    <param name="threshold" type="float" min="0" max="1" value="0.05" label="Q Value Threshold" />
  </inputs>
  <outputs>
    <data format="data" name="output" />
  </outputs>


  <tests>
    <test>
      <param name="input" value="fa_gc_content_input.fa"/>
      <output name="out_file1" file="fa_gc_content_output.txt"/>
    </test>
  </tests>         
  
  <help>
    This tool computes GC content from a FASTA file.
  </help>

</tool>


